[
	{
		"label": "Debug .NET Core App",
		"adapter": "netcoredbg",
		"request": "launch",
		"program": "$ZED_WORKTREE_ROOT/bin/Debug/net9.0/worldGen.dll",
		"cwd": "$ZED_WORKTREE_ROOT",
		"enableStepFiltering": true,
		"justMyCode": true,
		"build": {
			"command": "dotnet",
			"args": [
				"build",
				"$ZED_WORKTREE_ROOT/worldGen.csproj",
				"-p:Configuration=Debug",
				"-p:TargetFramework=net9.0"
			]
		}
	}
]
